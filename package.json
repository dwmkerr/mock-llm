{
  "name": "@dwmkerr/mock-llm",
  "version": "0.1.1",
  "description": "Simple OpenAI compatible Mock API server. Useful for deterministic testing of AI applications.",
  "main": "dist/server.js",
  "bin": {
    "mock-llm": "./dist/server.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "openai",
    "mock",
    "testing",
    "llm"
  ],
  "author": "Dave Kerr",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "jmespath": "^0.16.0",
    "js-yaml": "^4.1.0",
    "openai": "^4.28.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jmespath": "^0.15.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
