{
  "name": "@dwmkerr/mock-llm",
  "version": "0.1.28",
  "description": "Simple OpenAI compatible Mock API server. Useful for deterministic testing of AI applications.",
  "main": "dist/main.js",
  "bin": {
    "mock-llm": "./dist/main.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsx watch src/main.ts",
    "test-base": "jest",
    "local:inspect": "npx @modelcontextprotocol/inspector",
    "test": "jest --coverage --coverageDirectory=./artifacts/coverage --coverageReporters=text --coverageReporters=lcov --coverageReporters=text-summary --testPathIgnorePatterns=test-samples.spec.ts",
    "test:samples": "npm run build && jest --roots='<rootDir>' --testMatch='**/test-samples.spec.ts' --coverage --coverageDirectory=./artifacts/coverage-samples --coverageReporters=text --coverageReporters=lcov --coverageReporters=text-summary",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "openai",
    "mock",
    "testing",
    "llm"
  ],
  "author": "Dave Kerr",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwmkerr/mock-llm"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.4",
    "@modelcontextprotocol/sdk": "^1.20.1",
    "express": "^5.1.0",
    "jmespath": "^0.16.0",
    "js-yaml": "^4.1.0",
    "openai": "^4.28.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jmespath": "^0.15.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.48.0"
  }
}
